pipeline {
    agent any
    options {
        disableConcurrentBuilds()
    }
    stage('SCM Checkout') {
       git 'https://github.com/HariDEV-GIT/terra_test.git'
    }
    stage('Terraform Code Clean') {
       sh 'cd /var/lib/jenkins/workspace/test build'
       sh 'terraform fmt'
    }	     
}
